        {
          "id": 287981,
          "key": "4cf534af-99c6-45c1-be50-4b39be711614",
          "title": "Lab: Space Geek",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "4cf534af-99c6-45c1-be50-4b39be711614",
            "completed_at": null,
            "last_viewed_at": null,
