        {
          "id": 593931,
          "key": "5d87edd4-2f79-4fd8-9e34-f21118ec7b29",
          "title": "Sentiment Analysis with RNN",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "5d87edd4-2f79-4fd8-9e34-f21118ec7b29",
            "completed_at": null,
            "last_viewed_at": null,
